<?xml version="1.0" encoding="utf-8"?>
<resources
    xmlns:tools="http://schemas.android.com/tools"
    tools:ignore="MissingTranslation">
    <string name="album_name_all">Todas</string>

    <string name="button_preview">Visualizar</string>
    <string name="button_apply_default">Aplicar</string>
    <string name="button_apply">Aplicar(%1$d)</string>
    <string name="button_back">Voltar</string>
    <string name="photo_grid_capture">Câmera</string>
    <string name="empty_text">Nenhuma foto</string>
    <string name="button_ok">OK</string>

    <string name="error_over_count_default">Limite máximo de seleção alcançado</string>
    <plurals name="error_over_count">
        <item quantity="few">Você pode selecionar até %1$d arquivo</item>
        <item quantity="many">Você pode selecionar até %1$d arquivo</item>
        <item quantity="one">Você pode selecionar até 1 arquivo</item>
        <item quantity="other">Você pode selecionar até %1$d arquivos</item>
    </plurals>
    <string name="error_under_quality">Qualidade acima</string>
    <string name="error_over_quality">Qualidade abaixo</string>
    <string name="error_file_type">Tipo de arquivo não suportado</string>
    <string name="error_type_conflict">Não é possivel selecionar fotos e vídeos ao mesmo tempo</string>
    <string name="error_no_video_activity">Nenhum aplicativo encontrado para visualizar o vídeo</string>
    <string name="error_over_original_size">Não é possivel selecionar fotos maiores que %1$d MB</string>
    <string name="error_over_original_count">%1$d fotos acima de %2$d MB. Original será desmarcado</string>
    <string name="button_original">Original</string>
    <string name="button_sure_default">Selecionar</string>
    <string name="button_sure">Selecionar(%1$d)</string>
</resources>
